<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<?import java.net.URL?>
<VBox xmlns:fx="http://javafx.com/fxml"
          fx:controller="com.musicbox.bluetoothlatency.BTLatencyController"
          fx:id="mainPane" styleClass="mainPane">

            <Label styleClass="header" text="Select device"/>
            <HBox styleClass="hbox">
                <Label fx:id="textBox" text="Select device and press select." />
                <Button text="Select" onAction="#onProcessDeviceClick"/>
            </HBox>

            <ListView fx:id="discoveredListView"/>

<stylesheets><URL value="@stylesheet.css" /></stylesheets>
</VBox>
